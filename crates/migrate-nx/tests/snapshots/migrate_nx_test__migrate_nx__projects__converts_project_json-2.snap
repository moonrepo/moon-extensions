---
source: crates/migrate-nx/tests/migrate_nx_test.rs
expression: "fs::read_to_string(sandbox.path().join(\"baz/moon.yml\")).unwrap()"
---
fileGroups:
  default:
  - '**/*'
  production:
  - '!jest.config.ts'
  - '!**/?(*.)+(spec|test).ts'
language: javascript
platform: node
tasks:
  build:
    command: moon-nx execute @nx/js:tsc
    args:
    - --tsConfig
    - $workspaceRoot/baz/tsconfig.lib.json
    - --main
    - $workspaceRoot/baz/src/main.ts
    deps:
    - ^:build
    inputs:
    - '@group(production)'
    outputs:
    - /dist/baz
  build.production:
    extends: build
    args:
    - --tsConfig
    - $workspaceRoot/baz/tsconfig-prod.lib.json

